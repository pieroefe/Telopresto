<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TI.editar_equipo_usuarioti">

    <EditText
        android:id="@+id/editarIncluyeDispositivo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/editarStockDispositivo"
        tools:layout_editor_absoluteX="153dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Caracteristicas:"
        app:layout_constraintBottom_toBottomOf="@+id/editarCaracteristicaDispositivo"
        app:layout_constraintTop_toTopOf="@+id/editarCaracteristicaDispositivo"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="43dp" />

    <Spinner
        android:id="@+id/editarMarcaDispositivo"
        android:layout_width="222dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/editarCaracteristicaDispositivo"
        tools:layout_editor_absoluteX="145dp" />

    <EditText
        android:id="@+id/editarNombreDispositivo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:ems="8"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/editarMarcaDispositivo"
        tools:layout_editor_absoluteX="204dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Stock:"
        app:layout_constraintBottom_toBottomOf="@+id/editarStockDispositivo"
        app:layout_constraintTop_toTopOf="@+id/editarStockDispositivo"
        tools:layout_editor_absoluteX="52dp" />

    <EditText
        android:id="@+id/editarCaracteristicaDispositivo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/editarIncluyeDispositivo"
        tools:layout_editor_absoluteX="176dp" />

    <Button
        android:id="@+id/btn_editar_dispositivo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="161dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="16dp"
        android:text="Editar"
        app:layout_constraintBottom_toTopOf="@+id/editar_cancelar_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/editar_cancelar_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="148dp"
        android:layout_marginEnd="149dp"
        android:text="Cancelar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="634dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre del Dispositivo: "
        app:layout_constraintBottom_toBottomOf="@+id/editarNombreDispositivo"
        app:layout_constraintTop_toTopOf="@+id/editarNombreDispositivo"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="32dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Incluye:"
        app:layout_constraintBottom_toBottomOf="@+id/editarIncluyeDispositivo"
        app:layout_constraintTop_toTopOf="@+id/editarIncluyeDispositivo"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="52dp" />

    <EditText
        android:id="@+id/editarStockDispositivo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/btn_editar_dispositivo"
        tools:layout_editor_absoluteX="162dp" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginEnd="120dp"
        android:text="Editar Dispositivo"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="36dp" />

    <ImageView
        android:id="@+id/image_view_dispositivo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/editarNombreDispositivo"
        tools:layout_editor_absoluteX="141dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Marca:"
        app:layout_constraintBottom_toBottomOf="@+id/editarMarcaDispositivo"
        app:layout_constraintTop_toTopOf="@+id/editarMarcaDispositivo"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="57dp" />

</androidx.constraintlayout.widget.ConstraintLayout>